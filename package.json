{
  "name": "chek",
  "version": "1.0.18",
  "description": "Minimal utility for checking types, working with arrays and objects.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "node ./build/clean",
    "compile": "node ./node_modules/typescript/bin/tsc -p ./src/tsconfig.json",
    "browserify": "BROWSER=true node ./node_modules/browserify/bin/cmd  ./dist/index.js -o ./dist/browser.js",
    "uglify": "node ./node_modules/uglify-js/bin/uglifyjs ./dist/browser.js --output ./dist/browser.min.js --compress --mangle",
    "docs": "node ./node_modules/typedoc/bin/typedoc --out ./docs ./src --options ./typedoc.json --tsconfig ./src/tsconfig.json",
    "bump": "node ./build/bump",
    "build": "npm run clean && npm run compile && npm run browserify && npm run uglify && npm run docs",
    "commit": "git add . && git commit -am 'auto commit' && git push",
    "release": "npm run build && npm run bump && npm run commit && npm publish",
    "test": "nyc mocha --opts ./src/mocha.opts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/origin1tech/chek.git"
  },
  "keywords": [
    "Helpers",
    "Utils",
    "Lodash",
    "Underscore"
  ],
  "author": "Origin1 Technologies <origin1tech@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/origin1tech/chek/issues"
  },
  "homepage": "https://github.com/origin1tech/chek#readme",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/clone": "^0.1.30",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.7",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "deep-freeze": "0.0.1",
    "del": "^3.0.0",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "nyc": "^11.0.3",
    "ts-node": "^3.2.0",
    "tslint": "^5.4.3",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.1",
    "uglify-js": "^3.0.24"
  },
  "dependencies": {
    "clone": "^2.1.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "./build/spec"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}